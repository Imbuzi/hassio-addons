{
  "name": "DDclient",
  "version": "0.1.1",
  "slug": "ddclient",
  "description": "A Perl client used to update dynamic DNS entries for accounts on Dynamic DNS Network Service Provider.",
  "url": "https://github.com/nalipaz/hassio-addons/tree/master/ddclient",
  "startup": "system",
  "boot": "auto",
  "image": "nicholasalipaz/{arch}-ddclient",
  "options": {
    "protocol": "easydns",
    "remote_ip_utility_url": "support.easydns.com/utils/get_ip.php",
    "server": "members.easydns.com",
    "login": "username",
    "password": "password",
    "domains": [
      {"domain": "example.com"}
    ]
  },
  "schema": {
    "protocol": "str",
    "remote_ip_utility_url": "str",
    "server": "str",
    "login": "str",
    "password": "str",
    "domains": [
      {"domain": "str"}
    ]
  }
}
